#include <stdio.h>
#include <math.h>

int main() {
    int x, y;
    float z1, z2;
    printf("Enter you x and y\n");
    scanf("%d %d", &x, &y);
    
    z1 = pow(cos(x), 4) + pow(sin(y), 2) + 0.25*pow(sin(2*x-1), 2);
    z2 = sin(y + x) * sin(y - x);
    
    printf("%f \n%f", z1, z2);
    
    return 0;
}
