#include <stdio.h>
#include <math.h>

int main() {
    float x, y;
    double z1, z2;
    printf("Enter you x and y\n");
    scanf("%f %f", &x, &y);

    z1 = pow(cos(x), 4) + pow(sin(y), 2) + 0.25*pow(sin(2*x-1), 2);
    z2 = sin(y + x) * sin(y - x);

    printf("%lf \n%lf", z1, z2);

    return 0;
}
